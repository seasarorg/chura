<?xml version="1.0" encoding="UTF-8"?>
<templates>
	<template path="template/fm/scaffold/java_entity_s2dao.ftl">
		<output
			override="false"
			path="${javasrcroot}/${rootpackagepath}/${entitypackagename}/" 
			name="${table_capitalize}.java"/>
	</template>
	<template path="template/fm/scaffold/java_dao_s2dao.ftl">
		<output
			override="false"
			path="${javasrcroot}/${rootpackagepath}/${daopackagename}/" 
			name="${table_capitalize}${daosuffix}.java"/>
	</template>
	<template path="template/fm/scaffold/java_service_flex_s2dao.ftl">
