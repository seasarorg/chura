			override="true"
			path="${javasrcroot}/${rootpackagepath}/${subapplicationrootpackagename}/${table}/"
			name="${table_capitalize}${servicesuffix}.java"/>
	</template>
	<template path="template/fm/scaffold/java_service_flex_impl_s2dao.ftl">
