			path="${flexsrcroot}/${rootpackagepath}/${entitypackagename}/"
			name="${table_capitalize}.as" />
	</template>
	<template path="template/fm/scaffold/flex_page.ftl">
		<output 
			override="false"
			path="${flexsrcroot}/${rootpackagepath}/${subapplicationrootpackagename}/${table}/"
			name="${table_capitalize}${pagesuffix}.as" />
	</template>
	<template path="template/fm/scaffold/flex_mxml.ftl">
		<output 
