<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
  <extension-point id="resourceHandler" name="resourceHandler" schema="schema/resourceHandler.exsd"/>
  <extension-point id="newProject" name="newProject" schema="schema/newProject.exsd"/>
  <extension
      point="org.seasar.dolteng.projects.resourceHandler">
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.DefaultHandler"
        name="default"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.DefaultCounteractHandler"
        name="defaultCounteract"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.ClasspathHandler"
        name="classpath"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.ClasspathCounteractHandler"
        name="classpathCounteract"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.DoltengHandler"
        name="dolteng"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.DiiguHandler"
        name="diigu"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.TomcatHandler"
        name="tomcat"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.TomcatCounteractHandler"
        name="tomcatCounteract"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.WTPHandler"
        name="wtp"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.H2Handler"
        name="h2"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.JDTHandler"
        name="jdt"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.DbLauncherHandler"
        name="dblauncher"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.FlexBuilderHandler"
        name="flexbuilder"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.AppDiconHandler"
        name="appDicon"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.CustomizerDiconHandler"
        name="customizerDicon"/>
    <resourceHandler
        class="org.seasar.dolteng.projects.handler.impl.JMSDiconHandler"
        name="jmsDicon"/>
  </extension>
  <extension
      point="org.eclipse.ui.newWizards">
    <wizard
        canFinishEarly="false"
        category="org.seasar.dolteng.eclipse"
        class="org.seasar.dolteng.projects.wizard.ChuraProjectWizard"
        hasPages="true"
        icon="icons/chura.png"
        id="churaProjectWizard"
        name="Chura Project"
        project="true"/>
  </extension>
  <extension
      point="org.seasar.dolteng.projects.newProject">
<!-- ==== Seasar Base Template ==== -->
    <facet id="base" name="Base"
        root="template/resources,template/licenses,template/resources/data,template/jars,template/jars/sources"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <handler type="default">
        <entry kind="file" path="LICENSE.txt" />
        <entry kind="file" path="licenses/LICENSE.h2.html" />
        <entry kind="file" path="licenses/LICENSE.javassist.html" />
        <entry kind="file" path="licenses/LICENSE.junit.html" />
        <entry kind="file" path="licenses/LICENSE.ognl.txt" />
        <entry kind="file" path="licenses/NOTICE.txt" />
        <entry path="${mainJavaPath}/${packagePath}/dao" />
        <entry path="${mainJavaPath}/${packagePath}/dto" />
        <entry path="${mainJavaPath}/${packagePath}/dxo" />
        <entry path="${mainJavaPath}/${packagePath}/entity" />
        <entry path="${mainJavaPath}/${packagePath}/helper" />
        <entry path="${mainJavaPath}/${packagePath}/converter" />
        <entry path="${mainJavaPath}/${packagePath}/interceptor" />
        <entry path="${mainJavaPath}/${packagePath}/logic" />
        <entry path="${mainJavaPath}/${packagePath}/service" />
        <entry path="${mainJavaPath}/${packagePath}/validator" />
        <entry path="${mainJavaPath}/${packagePath}/web" />
        <entry path="${mainResourcePath}/${packagePath}/dao" />
        <entry path="${mainResourcePath}/${packagePath}/dto" />
        <entry path="${mainResourcePath}/${packagePath}/dxo" />
        <entry path="${mainResourcePath}/${packagePath}/entity" />
        <entry path="${mainResourcePath}/${packagePath}/helper" />
        <entry path="${mainResourcePath}/${packagePath}/converter" />
        <entry path="${mainResourcePath}/${packagePath}/interceptor" />
        <entry path="${mainResourcePath}/${packagePath}/logic" />
        <entry path="${mainResourcePath}/${packagePath}/service" />
        <entry path="${mainResourcePath}/${packagePath}/validator" />
        <entry path="${mainResourcePath}/${packagePath}/web" />
        <entry kind="file" path="${mainResourcePath}/convention.dicon" />
        <entry kind="file" path="${mainResourcePath}/creator.dicon" />
        <entry kind="file" path="${mainResourcePath}/env_ut.txt" />
        <entry kind="file" path="${mainResourcePath}/env.txt" />
        <entry kind="file" path="${mainResourcePath}/jdbc.dicon" />
        <entry kind="file" path="${mainResourcePath}/log4j.properties" />
        <entry kind="file" path="${mainResourcePath}/s2container.dicon" />
        <entry path="${testJavaPath}/${packagePath}/dao" />
        <entry path="${testJavaPath}/${packagePath}/dto" />
        <entry path="${testJavaPath}/${packagePath}/dxo" />
        <entry path="${testJavaPath}/${packagePath}/entity" />
        <entry path="${testJavaPath}/${packagePath}/helper" />
        <entry path="${testJavaPath}/${packagePath}/interceptor" />
        <entry path="${testJavaPath}/${packagePath}/logic" />
        <entry path="${testJavaPath}/${packagePath}/service" />
        <entry path="${testJavaPath}/${packagePath}/validator" />
        <entry path="${testResourcePath}/" />
      </handler>
      <handler type="classpath">
        <entry kind="output" path="${mainOutPath}" />
        <entry path="${mainJavaPath}" />
        <entry path="${mainResourcePath}" />
        <entry output="${testOutPath}" path="${testJavaPath}" />
        <entry output="${testOutPath}" path="${testResourcePath}" />
        <entry kind="con" path="${jreContainer}" />
        <entry kind="file" path="${testLibPath}/junit-3.8.2.jar" />
        <entry kind="file" path="${testLibPath}/junit-addons-1.4.jar" />
        <entry kind="file" path="${libPath}/aopalliance-1.0.jar" />
        <entry kind="file" path="${libPath}/commons-logging-1.1.jar" />
        <entry kind="file" path="${libPath}/javassist-3.4.ga.jar" />
        <entry kind="file" path="${libPath}/log4j-1.2.13.jar" />
        <entry kind="file" path="${libPath}/ognl-2.6.9-patch-20070908.jar" />
        <entry kind="file" path="${libPath}/poi-3.0-FINAL.jar" />
        <entry kind="file" path="${libPath}/h2-2007-12-27.jar" />
        <entry kind="file" path="${libPath}/s2-extension-2.4.20.jar" />
        <entry kind="file" path="${libPath}/s2-framework-2.4.20.jar" />
        <entry kind="file" path="${libPath}/geronimo-jta_1.1_spec-1.0.jar" />
        <entry kind="file" path="${testLibPath}/geronimo-servlet_2.4_spec-1.0.jar" />
      </handler>
      <handler type="jdt">
        <entry kind="file" path=".settings/jdt.pref" />
      </handler>
      <handler type="dolteng">
        <entry kind="file" path=".settings/dolteng.pref" />
      </handler>
      <handler type="dblauncher">
        <entry kind="file" path=".settings/dblauncher.pref" />
      </handler>
      <handler type="h2">
        <entry kind="file" path="${mainResourcePath}/data/demo.sql" />
      </handler>
      <handler type="appDicon">
        <include path="convention.dicon" />
        <include path="aop.dicon" />
      </handler>
      <handler type="customizerDicon">
        <include path="default-customizer.dicon" />
      </handler>
      <if jre="1.4" root="template/jdt/1.4" />
      <if jre="1.5,1.6" root="template/jdt/1.5">
        <handler type="classpath">
          <entry kind="file" path="${libPath}/s2-tiger-2.4.20.jar" />
        </handler>
      </if>
    </facet>
    <facet id="web" name="Web First"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <handler type="default">
        <entry path="${mainResourcePath}/${packagePath}/web" />
        <entry path="${mainJavaPath}/${packagePath}/web" />
        <entry path="${testJavaPath}/${packagePath}/web" />
      </handler>
    </facet>
    <facet id="standalone" name="Standalone First"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <property name="libPath" value="lib" />
      <property name="libSrcPath" value="lib/sources" />
      <property name="mainOutPath" value="target/classes" />
    </facet>

<!-- ==== Web Container Instance Management ==== -->
    <facet id="sysdeo" name="Sysdeo Tomcat Plugin"
        extends="base"
        root="template/sysdeo"
        displayOrder="SM01"
        description="%facetDesc.Sysdeo"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <if jre="1.4,1.5,1.6" />
      <handler type="default">
        <entry kind="file" path=".tomcatplugin" />
      </handler>
      <handler type="tomcat" />
    </facet>
    <facet id="WTP" name="WTP"
        extends="base"
        root="template/WTP"
        displayOrder="SM02"
        description="%facetDesc.WTP"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <if jre="1.5,1.6" />
      <handler type="classpath">
        <entry kind="con" path="org.eclipse.jst.j2ee.internal.web.container" />
      </handler>
      <handler type="wtp">
        <entry kind="file" path=".settings/org.eclipse.jst.common.project.facet.core.prefs" />
        <entry kind="file" path=".settings/org.eclipse.wst.common.component" />
        <entry kind="file" path=".settings/org.eclipse.wst.common.project.facet.core.xml" />
      </handler>
    </facet>

<!-- ==== Presentation.Java Layer Support ==== -->
    <facet id="teeda" name="Teeda Base"
        extends="base"
        root="template/teeda/WEB-INF,template/teeda"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <property name="mainJavaPath" value="src/main/java" />
      <property name="mainResourcePath" value="src/main/resources" />
      <property name="testJavaPath" value="src/test/java" />
      <property name="testResourcePath" value="src/test/resources" />
      <property name="mainOutPath" value="src/main/webapp/WEB-INF/classes" />
      <property name="testLibPath" value="lib" />
      <property name="libPath" value="src/main/webapp/WEB-INF/lib" />
      <handler type="default">
        <entry kind="file" path="${mainJavaPath}/${packagePath}/web/error/ErrorPage.java" />
        <entry kind="file" path="${mainResourcePath}/appMessages.properties" />
        <entry kind="file" path="${mainResourcePath}/appMessages_ja.properties" />
        <entry kind="file" path="${mainResourcePath}/app_aop.dicon" />
        <entry kind="file" path="${mainResourcePath}/teedaErrorPage.dicon" />
        <entry kind="file" path="${mainResourcePath}/teedaCustomize.dicon" />
        <entry kind="file" path="${webAppRoot}/index.jsp" />
        <entry kind="file" path="${webAppRoot}/view/error/error.html" />
        <entry kind="file" path="${webAppRoot}/WEB-INF/faces-config.xml" />
        <entry kind="file" path="${webAppRoot}/WEB-INF/web.xml" />
      </handler>
      <handler type="classpath">
        <entry output="${testOutPath}" path="${webAppRoot}/view" />
        <entry kind="file" path="${libPath}/teeda-ajax-1.0.11-SP3.jar" />
        <entry kind="file" path="${libPath}/teeda-core-1.0.11-SP3.jar" />
        <entry kind="file" path="${libPath}/teeda-extension-1.0.11-SP3.jar" />
        <entry kind="file" path="${libPath}/commons-collections-3.1.jar" />
        <entry kind="file" path="${libPath}/commons-el-1.0.jar" />
        <entry kind="file" path="${libPath}/commons-fileupload-1.0.jar" />
        <entry kind="file" path="${libPath}/jstl-1.1.2.jar" />
        <entry kind="file" path="${libPath}/tomahawk-1.1.1.jar" />
        <entry kind="file" path="${testLibPath}/rhino-1.6r2.jar" />
        <entry kind="file" path="${testLibPath}/teeda-xmlunit-0.1.0.jar" />
        <entry kind="file" path="${testLibPath}/teeda-webunit-0.2.2.jar" />
        <entry kind="file" path="${libPath}/xercesImpl-2.6.2.jar" />
        <entry kind="file" path="${libPath}/xmlParserAPIs-2.6.2.jar" />
        <entry kind="file" path="${testLibPath}/xmlunit-1.0.jar" />
        <entry kind="file" path="${testLibPath}/geronimo-jsp_2.0_spec-1.0.jar" />
      </handler>
      <handler type="dolteng">
        <entry kind="file" path=".settings/dolteng.teeda.pref" />
      </handler>
      <handler type="appDicon">
        <include path="app_aop.dicon" />
        <include path="teedaExtension.dicon" />
        <include path="dxo.dicon" />
      </handler>
      <handler type="customizerDicon">
        <component name="pageCustomizer">
          <addCustomizer name="traceCustomizer" />
          <addCustomizer name="AspectCustomizer" aspect="app_aop.appFacesExceptionThrowsInterceptor" />
        </component>
        <component name="actionCustomizer">
          <addCustomizer name="traceCustomizer" />
          <addCustomizer name="AspectCustomizer" aspect="app_aop.actionSupportInterceptor" />
        </component>
        <component name="serviceCustomizer">
          <addCustomizer name="traceCustomizer" />
          <addCustomizer name="requiredTxCustomizer" />
        </component>
        <component name="logicCustomizer">
          <addCustomizer name="traceCustomizer" />
        </component>
        <component name="daoCustomizer">
          <addCustomizer name="traceCustomizer" />
        </component>
        <component name="dxoCustomizer">
          <addCustomizer name="traceCustomizer" />
          <addCustomizer name="s2DxoCustomizer" />
        </component>
        <component name="helperCustomizer">
          <addCustomizer name="traceCustomizer" />
        </component>
      </handler>
      <if jre="1.4" />
      <if jre="1.5,1.6">
        <handler type="default">
          <entry kind="file" path="pom.xml" />
        </handler>
        <handler type="classpath">
          <entry kind="file" path="${libPath}/teeda-tiger-1.0.11-SP3.jar" />
        </handler>
      </if>
    </facet>
    <facet id="teedaPage" name="Teeda (Page only)"
        extends="teeda"
        root="template/teeda/WEB-INF,template/teeda"
        displayOrder="PR01"
        description="%facetDesc.Teeda"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <handler type="customizerDicon">
        <component name="pageCustomizer">
          <addCustomizer name="AspectCustomizer" aspect="j2ee.requiredTx" />
        </component>
      </handler>
      <if jre="1.4,1.5,1.6" />
    </facet>
    <facet id="teedaAction" name="Teeda (with Action)"
        extends="teeda"
        root="template/teeda/WEB-INF,template/teeda"
        displayOrder="PR02"
        description="%facetDesc.Teeda"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <handler type="customizerDicon">
        <component name="actionCustomizer">
          <addCustomizer name="AspectCustomizer" aspect="j2ee.requiredTx" />
        </component>
      </handler>
      <if jre="1.4,1.5,1.6" />
    </facet>
    <facet id="s2wicket" name="S2Wicket"
        extends="base"
        root="template/s2wicket"
        displayOrder="PR04"
        description="%facetDesc.S2Wicket"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <handler type="default">
        <entry path="src/main/html/${packagePath}/web" />
        <entry kind="file" path="${mainJavaPath}/${packagePath}/web/Application.java" />
        <entry kind="file" path="${mainJavaPath}/${packagePath}/web/HelloWorld.java" />
        <entry kind="file" path="src/main/html/${packagePath}/web/HelloWorld.html" />
        <entry kind="file" path="${webAppRoot}/WEB-INF/web.xml" />
      </handler>
      <handler type="classpath">
        <entry path="src/main/html" />
        <entry kind="file" path="${libPath}/wicket-1.2.4.jar" />
        <entry kind="file" path="${libPath}/s2wicket-1.2.1.jar" />
        <entry kind="file" path="${libPath}/cglib-nodep-2.1_3.jar" />
      </handler>
      <handler type="dolteng">
        <entry kind="file" path=".settings/dolteng.s2wicket.pref" />
      </handler>
      <if jre="1.5,1.6" />
    </facet>
    <facet id="s2flex2" name="S2Flex2"
        extends="base"
        root="template/s2flex2"
        displayOrder="PR05"
        description="%facetDesc.S2Flex2"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <property name="mainJavaPath" value="WEB-INF/src/main/java" />
      <property name="mainResourcePath" value="WEB-INF/src/main/resources" />
      <property name="testJavaPath" value="WEB-INF/src/test/java" />
      <property name="testResourcePath" value="WEB-INF/src/test/resources" />
      <property name="mainOutPath" value="WEB-INF/classes" />
      <property name="testLibPath" value="WEB-INF/lib" />
      <property name="libPath" value="WEB-INF/lib" />
      <property name="webAppRoot" value="" />
      <handler type="default">
        <entry kind="file" path="Main.mxml" />
        <entry path="${packagePath}/dto" />
        <entry path="${packagePath}/web" />
        <entry kind="file" path="${mainResourcePath}/amf3.dicon" />
        <entry kind="file" path="${mainResourcePath}/remoting_amf3.dicon" />
        <entry kind="file" path="WEB-INF/web.xml" />
      </handler>
      <handler type="classpath">
        <entry kind="file" path="${libPath}/s2-flex2-1.0.2.jar" />
        <entry kind="file" path="${libPath}/geronimo-jsp_2.0_spec-1.0.jar" />
      </handler>
      <handler type="dolteng">
        <entry kind="file" path=".settings/dolteng.s2flex2.pref" />
      </handler>
      <handler type="appDicon">
        <include path="remoting_amf3.dicon" />
      </handler>
      <handler type="customizerDicon">
        <component name="remotingServiceCustomizer"
            class="org.seasar.flex2.rpc.remoting.service.customizer.RemotingServiceCustomizer" />
      </handler>
      <handler type="flexbuilder">
        <entry kind="file" path=".actionScriptProperties" />
        <entry kind="file" path=".flexProperties" />
        <entry kind="file" path="html-template/AC_OETags.js" />
        <entry kind="file" path="html-template/history.htm" />
        <entry kind="file" path="html-template/history.js" />
        <entry kind="file" path="html-template/history.swf" />
        <entry kind="file" path="html-template/index.template.html" />
        <entry kind="file" path="html-template/playerProductInstall.swf" />
        <entry kind="file" path="WEB-INF/lib/swc/s2flex2-components.swc" />
      </handler>
      <if jre="1.4" />
      <if jre="1.5,1.6">
        <handler type="classpath">
          <entry kind="file" path="WEB-INF/lib/s2-tiger-2.4.20.jar" />
          <entry kind="file" path="WEB-INF/lib/s2-dao-tiger-1.0.47.jar" />
          <entry kind="file" path="WEB-INF/lib/geronimo-ejb_3.0_spec-1.0.jar" />
          <entry kind="file" path="WEB-INF/lib/s2-flex2-tiger-1.0.0.jar" />
        </handler>
      </if>
    </facet>

<!-- ==== Persistence.Java ==== -->
    <facet id="s2dao" name="S2Dao" root="template/s2dao"
        extends="base"
        displayOrder="PE01"
        description="%facetDesc.S2Dao"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <handler type="classpath">
        <entry kind="file" path="${libPath}/s2-dao-1.0.47.jar" />
      </handler>
      <handler type="dolteng">
        <entry kind="file" path=".settings/dolteng.s2dao.pref" />
      </handler>
      <handler type="appDicon">
        <include path="dao.dicon" />
        <include path="dxo.dicon" />
      </handler>
      <handler type="customizerDicon">
        <component name="serviceCustomizer">
          <addCustomizer name="traceCustomizer" />
          <addCustomizer name="requiredTxCustomizer" />
        </component>
        <component name="logicCustomizer">
          <addCustomizer name="traceCustomizer" />
        </component>
        <component name="daoCustomizer">
          <addCustomizer name="traceCustomizer" />
          <addCustomizer name="s2DaoCustomizer" />
        </component>
        <component name="dxoCustomizer">
          <addCustomizer name="traceCustomizer" />
          <addCustomizer name="s2DxoCustomizer" />
        </component>
        <component name="helperCustomizer">
          <addCustomizer name="traceCustomizer" />
        </component>
      </handler>
      <if jre="1.4" />
      <if jre="1.5,1.6">
        <handler type="classpath">
          <entry kind="file" path="${libPath}/s2-dao-tiger-1.0.47.jar" />
        </handler>
      </if>
    </facet>
    <facet id="kuina" name="Kuina Base"
        extends="base"
        root="template/kuina"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <property name="libPath" value="lib" />
      <property name="libSrcPath" value="lib/sources" />
      <property name="mainOutPath" value="target/classes" />
      <handler type="default">
        <entry kind="file" path="${mainResourcePath}/META-INF/services/javax.xml.parsers.SAXParserFactory" />
        <entry kind="file" path="${testResourcePath}/s2junit4.dicon" />
      </handler>
      <handler type="classpath">
        <entry kind="file" path="${libPath}/antlr-2.7.6.jar" />
        <entry kind="file" path="${libPath}/commons-collections-3.1.jar" />
        <entry kind="file" path="${libPath}/commons-lang-2.1.jar" />
        <entry kind="file" path="${libPath}/dom4j-1.6.1.jar" />
        <entry kind="file" path="${libPath}/ehcache-1.2.jar" />
        <entry kind="file" path="${libPath}/geronimo-annotation_1.0_spec-1.0.jar" />
        <entry kind="file" path="${libPath}/geronimo-ejb_3.0_spec-1.0.jar" />
        <entry kind="file" path="${libPath}/geronimo-interceptor_3.0_spec-1.0.jar" />
        <entry kind="file" path="${libPath}/geronimo-jpa_3.0_spec-1.0.jar" />
        <entry kind="file" path="${libPath}/kuina-dao-1.0.1.jar" />
        <entry kind="file" path="${testLibPath}/junit-4.3.1.jar" />
      </handler>
      <handler type="diigu" />
      <handler type="dolteng">
        <entry kind="file" path=".settings/dolteng.kuina.pref" />
      </handler>
      <handler type="appDicon">
        <include path="kuina-dao.dicon" />
        <include path="dxo.dicon" />
      </handler>
      <handler type="customizerDicon">
        <component name="serviceCustomizer">
          <addCustomizer name="traceCustomizer" />
          <removeCustomizer name="requiredTxCustomizer" />
        </component>
        <component name="logicCustomizer">
          <addCustomizer name="traceCustomizer" />
        </component>
        <component name="daoCustomizer">
          <addCustomizer name="traceCustomizer" />
          <addCustomizer name="kuinaDaoCustomizer" />
        </component>
        <component name="dxoCustomizer">
          <addCustomizer name="traceCustomizer" />
          <addCustomizer name="s2DxoCustomizer" />
        </component>
        <component name="helperCustomizer">
          <addCustomizer name="traceCustomizer" />
        </component>
      </handler>
    </facet>
    <facet id="kuinaHibernate" name="Kuina-Dao (Hibernate)"
        extends="kuina"
        root="template/kuina/hibernate"
        displayOrder="PE02"
        description="%facetDesc.KuinaHibernate"
        resourceLoader="org.seasar.dolteng.eclipse.loader.impl.CompositeResourceLoader">
      <if jre="1.5,1.6" />
